<form id="form-more" class="mt-3 needs-validation">
  <fieldset>
    <input name="yearsoldmore" type="hidden" class="form-control" id="yearsoldmore" placeholder="Name" value="1">
    <input name="contractmore" type="hidden" class="form-control" id="contractmore" placeholder="contract" value="0">
    <div class="row">
      <div class="col-md-3 mb-3">
        <div class="form-group">
          <label for="citymore">Місто *</label>
          <select name="citymore" class="form-control" id="citymore">
            <option value="Київ">Оберіть місто *</option>
          </select>
        </div>
      </div>
      <div class="col-md-9 mb-3">
        <div class="form-group">
          <label for="centermore">Центр тестування *</label>
          <select name="centermore" class="form-control" id="centermore">
            <option value="00000">Оберіть центр тестування *</option>
          </select>
        </div>
      </div>
    </div>
    {{!-- <div class="form-group">
      <label for="centermore">Центр тестування</label>
      <select name="centermore" class="form-control" id="centermore">
        <option value="00000">Оберіть центр тестування</option>
      </select>
    </div> --}}
    <div class="row">
      <div class="col-md-6 mb-3">
        <label for="firstnamemore">І'мя (ВЕЛИКИМИ латинськими літерами, для відображення в сертифікаті) *</label>
        <input name="firstnamemore" type="text" class="form-control" id="firstnamemore" placeholder="Name" value="" required="">
        <div class="invalid-feedback">
          Введіть імя.
        </div>
      </div>
      <div class="col-md-6 mb-3">
        <label for="lastnamemore">Прізвище (ВЕЛИКИМИ латинськими літерами, для відображення в сертифікаті) *</label>
        <input name="lastnamemore" type="text" class="form-control" id="lastnamemore" placeholder="Last Name" value=""
          required="">
        <div class="invalid-feedback">
          Введіть Прізвище.
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-md-3 mb-3">
        <label for="birthmore">Дата народження *</label>
        <input name="birthmore" type="text" class="form-control" id="birthmore" placeholder="15.10.2020" value="" required="">
        <div class="invalid-feedback">
          Введіть дату народження.
        </div>
      </div>
      <div class="col-md-5 mb-3">
        <div class="form-group">
          <label for="levelmore">Рівень іспиту PTE *</label>
          <select name="levelmore" class="form-control" id="levelmore" required="">
            <option value="0" selected="selected">Оберіть рівень *</option>
          </select>
        </div>
      </div>
        <div class="col-md-4 mb-3">
          <div class="form-group">
            <label for="costmore">Вартість обраного іспиту(грн) *</label>
            <input name="costmore" type="text" class="form-control" id="costmore" placeholder="Оберіть рівень іспиту" value="0" readonly>
          </div>
        </div>
    </div>

    <div class="row">
      <div class="col-md-6 mb-3">
        <label for="emailmore">E-mail *</label>
        <input name="emailmore" type="emailmore" class="form-control" id="emailmore" aria-describedby="emailHelp"
          placeholder="Ваш email">
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone
          else.</small>
      </div>
      <div class="col-md-6 mb-3">
        <div class="form-group">
          <label for="phonemore">Телефон *</label>
          <input name="phonemore" type="text" class="form-control" id="phonemore" placeholder="+38096 345 1234" value=""
            required="">
          <div class="invalid-feedback">
            Введіть номер телефону.
          </div>
        </div>
      </div>
    </div>

    <div class="row">
      <div class="col-md-12">
        <p>Екзаменаційна сесія *</p>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label for="monthmore">Місяць</label>
          <select name="monthmore" class="form-control" id="monthmore">
            <option value="03">Березень</option>
            <option value="05">Травень</option>
            <option value="06">Червень</option>
            <option value="11">Листопад</option>
            <option value="12">Грудень</option>
          </select>
        </div>
      </div>
      <div class="col-md-6">
        <div class="form-group">
          <label for="yearmore">Рік *</label>
          <select name="yearmore" class="form-control" id="yearmore">
            <option>2020</option>
          </select>
        </div>
      </div>
    </div>
    <fieldset class="form-group">
      <legend>Погодження</legend>
      <div class="form-check">
        <label class="form-check-label">
          <input class="form-check-input" type="checkbox" value="" required="">
          Ознайомлений з <span class="oferta-btn" data-toggle="modal" data-target=".oferta-modal-lg">договором публічної оферти</span>
        </label>
      </div>
    </fieldset>
    <button type="submit" class="btn btn-primary sbm">Зареєструватись</button>
  </fieldset>
  <p class="mt-3"></p>
</form>